import java.util.Arrays;

public class Main {
    public static void main(String[] args) {
        Algs alg = new Algs();
        String[] anna = {"Zebra", "alpha"};
        String[] annabeth = {"Zebra", "alpha"};
        String[] bigThree = {"Zeus" , "Poseidon", "Hades"};
        String[] godesses = {"Athena", "Demeter", "Persephone", "Hera"};
        alg.selectionSort(anna);
        System.out.println();
        alg.selectionSort(bigThree);
        System.out.println();
        alg.selectionSort(godesses);
        System.out.println();


        alg.insertionSort(annabeth);
    }

}
