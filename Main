        String[] monsters = {"Minotaur", "cyclops", "hydra", "Chimera", "Sphyinx"};
        //selection sort
        System.out.println("Selection sort and insertion sort with arrays");
        alg.print(anna);
        alg.selectionSort(anna);
        System.out.println();

        alg.print(bigThree);
        alg.selectionSort(bigThree);
        System.out.println();

        alg.print(godesses);
        alg.selectionSort(godesses);
        System.out.println();

        //insertion sort
        alg.print(annabeth);
        alg.insertionSort(annabeth);
        System.out.println();

        alg.print(heroes);
        alg.insertionSort(heroes);
        System.out.println();

        alg.print(monsters);
        alg.insertionSort(monsters);
        System.out.println();

